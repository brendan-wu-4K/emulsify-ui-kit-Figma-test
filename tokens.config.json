{
  "platforms": {
    "swift": {
      "transformGroup": "ios-swift",
      "buildPath": "./ios/",
      "files": [
        {
          "destination": "ios",
          "format": "ios-swift/enum.swift",
          "options": {
            "fileHeader": "This is a custom header"
          }
        }
      ]
    },
    "android": {
      "transformGroup": "android",
      "files": [
        {
          "destination": "android",
          "format": "android/resources",
        }
      ]
    }
  }
}